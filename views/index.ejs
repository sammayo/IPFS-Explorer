<!DOCTYPE html>
<html>
	<head>
		<title>IPFS Explorer</title>
		<script src="/node_modules/jquery/dist/jquery.min.js"></script>
		<link href="/node_modules/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
		<script src="/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
		<link rel="stylesheet" href="/node_modules/tingle.js/dist/tingle.min.css">
		<script src="/node_modules/tingle.js/dist/tingle.min.js"></script>
		<link rel="stylesheet" href="/node_modules/backgrid/lib/backgrid.css"/>
		<script src="/node_modules/underscore/underscore-min.js"></script>
		<script src="/node_modules/backbone/backbone-min.js"></script>
    	<script src="/node_modules/backgrid/lib/backgrid.js"></script>
    	<!-- <script src="/bundle.js"></script> -->
		<link href="/css/uploadbutton.css" rel="stylesheet">
		<script src="/js/uploadbutton.js"></script>
		<link href="/css/main.css" rel="stylesheet">
		<script src="/js/download.js"></script>
		<script src="/js/FileSaver.min.js"></script>
		<script src="/js/bundle.js"></script>
	</head>
	<body>
		<div class="row">
			<div class="col-md-4"></div>
			<div class="col-md-4">
				<h1>IPFS Explorer</h1>
			</div>
			<div class="col-md-4"></div>
		</div>
		<div class="row">
			<div class="col-md-4"></div>
			<div class="col-md-4">
				<div id="div-upload">
					<div class="fileupload fileupload-new" data-provides="fileupload">
					    <span class="btn btn-primary btn-file">
					    <span class="fileupload-new">Choose a file</span>
					    <span class="fileupload-exists">Change file</span>
					    <input id="in-file" type="file"/></span>
					    <span class="fileupload-preview"></span>
					    <a href="#" id="dismiss-in-file" class="close fileupload-exists" data-dismiss="fileupload" style="float: none">Ã—</a>
		 			</div>
				</div>
			</div>
			<div class="col-md-4">
				<button id="btn-file-submit" class="btn btn-info">Upload</button>
			</div>
		</div>
		<div class="row">
			<div class="col-md-3"></div>
			<div class="col-md-6">
				<form class="navbar-form">
			        <div class="form-group" style="display:inline;">
			          	<div class="input-group" style="display:table;">
			            	<input id="search-hash" class="form-control" name="search" placeholder="Enter hash of file to download" autocomplete="off" autofocus="autofocus" type="text">
			            	<span class="input-group-addon" style="width:1%;">
			            		<span id="btn-search-hash" class="glyphicon glyphicon-search"></span>
			            	</span>
			          	</div>
			        </div>
			    </form>
			</div>
			<div class="col-md-3"></div>
		</div>
		<div class="row">
			<div class="col-md-2"></div>
			<div class="col-md-8">
				<div id="view-downloadables"></div>
			</div>
			<div class="col-md-2"></div>
		</div>
		<script src="/js/main.js"></script>s
	</body>
</html>